<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.fxft.cheyoufuwu.ui.login.activity.RegisteDetailActivity">

    <com.fxft.cheyoufuwu.common.view.CommonTopBar
        android:id="@+id/ctb_registe_top_bar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/white"
        app:left_custom_layout="@layout/common_top_left_back_button_black_layout"
        app:left_layout_enable="true"
        app:title_enable="true"
        app:title_text="@string/registe"
        app:title_text_color="@color/black"
        app:title_text_size="@dimen/middle_text_size" />

    <EditText
        android:id="@+id/et_input_user_nickname"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_10dp"
        android:background="@color/white"
        android:hint="@string/input_user_nickname"
        android:paddingBottom="@dimen/padding_10dp"
        android:paddingLeft="20dp"
        android:paddingTop="@dimen/padding_10dp"
        android:textSize="@dimen/default_text_size" />

    <EditText
        android:id="@+id/input_psw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/padding_10dp"
        android:background="@color/white"
        android:hint="@string/input_psw"
        android:paddingBottom="@dimen/padding_10dp"
        android:paddingLeft="20dp"
        android:paddingTop="@dimen/padding_10dp"
        android:textSize="@dimen/default_text_size" />

    <EditText
        android:id="@+id/submit_psw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:hint="@string/submit_psw"
        android:paddingBottom="@dimen/padding_10dp"
        android:paddingLeft="20dp"
        android:paddingTop="@dimen/padding_10dp"
        android:textSize="@dimen/default_text_size" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/padding_10dp"
        android:paddingLeft="20dp"
        android:paddingTop="@dimen/padding_10dp"
        android:text="@string/psw_input_rule"
        android:textColor="@color/gray_text"
        android:textSize="@dimen/middle_text_size" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:padding="20dp">

        <CheckBox
            android:id="@+id/cb_check_readed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:button="@drawable/common_red_check_box_bg" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/padding_10dp"
            android:text="@string/submit_protocal" />

        <TextView
            android:id="@+id/tv_cheyoufuwu_protocal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4dp"
            android:text="@string/cheyoufu_protocal"
            android:textColor="@color/light_red" />

    </LinearLayout>

    <Button
        android:id="@+id/bt_finish_regist_button"
        android:layout_width="match_parent"
        android:layout_height="45.0dip"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/common_red_radius_button_bg"
        android:text="@string/finish_regist"
        android:textColor="#ffffffff"
        android:textSize="18.0sp" />


</LinearLayout>
